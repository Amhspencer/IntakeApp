                            <div class="module">
                              <div class="module-head">
                                <h3 id="unprocessed_table">
                                  Unprocessed Forms
                                    <% if @user.type == "Partner" %>
                                      <small>Your previously submitted intake forms</small>
                                    <% end %>
                                </h3>
                              </div>
                              <div class="module-body table">
                                <center>
                                <table cellpadding="0" cellspacing="0" border="0"  width="99%" class="datatable-1 table table-bordered table-striped  display" id="unprocessed_forms" name="unprocessed_forms">
                                  <thead>
                                    <tr>
                                      <th id="unproc_name">Name</th>
                                      <th id="unproc_date">Date Submitted</th>
                                      <th id="unproc_facility">Medical Facility</th>
                                      <th id="unproc_county">County</th>
                                      <th id="unproc_language">Preferred Language</th>
                                    </tr>
                                  </thead>
                                  <tbody>
                                    <% @unprocessedForms.each do |form| %>
                                      <tr>
                                        <td><%= link_to "#{form.last_name}, #{form.first_name[0]}", showToProcess_form_path(form) %></td>
                                        <td><%= form.created_at.strftime("%m/%d/%y") %></td>
                                        <td><%= form.medical_facility %></td>
                                        <td><%= form.service_area %></td>
                                        <td><%= form.preferred_language %></td>
                                      </tr>
                                    <% end %>
                                  </tbody>
                                </table>
                                </center>
                              </div>
                            </div><!--/.module-->                            